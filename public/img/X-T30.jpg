              quantity = parseInt($('[data-id="' + id_item +'"]').find('.quantity').text().trim());
              quantity = quantity;
          }
          var id_item_check = id_item.replace(/[^0-9]/g, '').trim();
          if(id_item_check  == number){
            $(this).find('input[type=checkbox]').attr('checked', false);
        } 
        find_id(id_item, title_item, price_item, img_item, quantity);
    }
});
   }
}

function create_tr (id_item, title_item, price_item, img_item, quantity, quantity_step, upgrades) {
        var delete_btn_text = "";
        
    var id_suffix = id_item.match(/\d+/);
    id_suffix = "/items/" + parseInt(id_suffix);
    $('div.cart #main_cart table').prepend('<tr data-id="' + id_item + '"><td><div class="quantity_step_value_in_cart" style="display: none;">' + quantity_step + '</div><div class="quantity">' + quantity + '</div><div class="cart_small_button"><a class="reduce">-</a><a class="plus">+</a></div></td><td class="img_item"><img no-lazy data-lazy="false" src="' + img_item + '"></td><td class="title"><a href="' + id_suffix + '">' + title_item + '</a></td><td class="delete_btn"><a>'+delete_btn_text+'</a></td><td class="price_item_x">' + price_item + '</td></tr>');
    if(upgrades != "" && upgrades != null && upgrades != undefined) {
        $('[data-id="' + id_item +'"] .price_item_x').before('<td class="upgrades">' + upgrades + '</td>');
    }
    $('div.cart_small_button a').unbind();
    $('[data-id="' + id_item +'"]').css({
        'height': 'auto',
        'max-height': '999px'
    }).animate({'max-height':'999px'},200);
    delete_btn_div();
    cart_quantity();
    sumAllItems();
    setAllCartTojStorage();
    fix_remove_images();
    step_vals();
    item_in_cart_changed($('div.cart #main_cart table tr[data-id="' + id_item + '"]'));
    setTimeout(function () { height_cart();}, 200);
}
function create_table () {
    $('div.cart #main_cart').prepend('<table></table>');
    setAllCartTojStorage();
}
function delete_item (id_item) {
    var item_class = '[data-id="' + id_item + '"]';
    var self = $('div#main_cart table').find(item_class);
    self.animate({height:0},200);
    setTimeout(function () { self.remove(); setAllCartTojStorage(); sumAllItems(); setAllCartTojStorage();}, 200);
    setTimeout(function () { height_cart();}, 200);
}

function update_item (id_item, quantity) {
    var item_class = '[data-id="' + id_item + '"]';
    var check_quantity = $('div#main_cart table').find(item_class).children().children('.quantity').text();
    if (check_quantity>0) {
        $('div.cart #main_cart table tr' + item_class + ' .quantity').text(quantity);
    }
    check_grid();
    sumAllItems();
    setAllCartTojStorage();
}

function setAllCartTojStorage () {
    var all_cart = $('div.cart #main_cart table tbody').html();
    $.jStorage.set($('#cart_key').val(),all_cart);
    $('span.cart_with_items_counter').text($('.special_cart div.count > div span.pos_0').text());
    cart_flash_light();
}
function submit_checkout () {
    if ($('.total_all_items').text()=="0") {    
        $('.special_cart div.checkout a').unbind("click");
    }
   else {
        $('div.checkout a').click(function(e) {
            $('.special_cart div#main_cart table tr').each(function(index, el) {
                if ($(this).find('.quantity').text() == '0') {
                  $(this).remove();

              };
          });
        if ($('.total_all_items').text()=="0") {    
            $('.empty_cart a').click();
            $('.special_cart div.checkout a').unbind("click");
            e.preventDefault();
            e.stopPropagation();
        }
else{
            e.preventDefault();
            set_cart_content(); 
            $(this).prev().children("#cart_content").val(finish_cart_details).attr('name','cart_content_with_upgrades');
            $(this).prev().children("#referer_url").val($('meta[name="referer_url"]').attr('content'));
            $(this).prev().children("#request_url").val($('meta[name="request_url"]').attr('content'));
            $(this).prev().children("#ip_address").val($('meta[name="ip_address"]').attr('content'));
            $(this).prev().children("#single_access_token").val($('meta[name="single_access_token"]').attr('content'));
            $('#flying_cart').submit();
}
        });
    }
}
function delete_btn_div (argument) {
    $('.delete_btn').children().click(function() {
        var self = $(this).parent().parent();
        var cart_tr_ids = $(this).parent().parent().attr("data-id");
        var self_id = $(this).parent().parent().find('td.title a').attr('href').split('/')[2];
        self.animate({height:0},200);
        setTimeout(function () { self.remove(); setAllCartTojStorage();}, 200);
        setTimeout(function () { height_cart(); sumAllItems(); $('span.cart_with_items_counter').text($('.special_cart div.count > div span.pos_0').text()); }, 200);
        $('div#matchingCarouselHook em').each(function(){
          if($(this).find('input.old_checkbox').val() == self_id){
           if ($(this).find('input.checkbox_for_add').is(':checked')) {
            $(this).removeClass('active');
            $(this).find('input.checkbox_for_add').click();
            $(this).find('input.old_checkbox').click();
        }
    }
});
$('.special_cart.layout_item a.commit_to_real').removeClass('clicked');
        if($('.layout_list_item').length > 0)
            update_grid_to_zero(cart_tr_ids);
    });
}
function layout_order_convert_json_to_string(arr_of_json) {
    if(arr_of_json.length > 0) {
        var ret_string = "", tmp_upg_arr;
        $.each(arr_of_json, function(index, json) {
            ret_string += "item_id_" + json.item_id + "=>" + json.quantity;
            tmp_upg_arr = [];
            if(json.item_upgrade_ids != null) {
                $.each(json.item_upgrade_ids, function (index, upgrade) {
                    tmp_upg_arr.push('u‖'+upgrade.toString());
                });
            }
            if(json.options != null) {
                $.each(json.options, function(option_name, option_value) {
                    tmp_upg_arr.push(option_name + "‖" + option_value);
                });
            }
if(tmp_upg_arr.length > 0) { // runs only if the product have upgrades
    ret_string += "=>";
    tmp_upg_arr.sort();
    ret_string += tmp_upg_arr.join('»'); //delimiter for each upgrade
}
ret_string += "•"; //delimiter for each product
});
        return encodeURI(ret_string);
    } else
    console.log("function: layout_order_convert_json_to_string, err: no array was entered");
}
