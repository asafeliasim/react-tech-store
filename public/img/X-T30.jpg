<script>

function check_for_no_item_changed_class() {
    $('tr.item_changed').removeClass('item_changed');
}

function find_id (id_item, title_item, price_item, img_item, quantity, quantity_step, upgrades) {
    if($('div#main_cart table').html()){
        var item_class = '[data-id="' + id_item + '"]';
        if ($('div#main_cart table').find(item_class).length > 0) {
            if (quantity <= 0) {
                delete_item(id_item); 
            }
            else {
                update_item(id_item, quantity);
                item_in_cart_changed($('div#main_cart table').find(item_class).first());
            }   
        }
        else {  
            if (quantity > 0){
                create_tr(id_item, title_item, price_item, img_item, quantity, quantity_step, upgrades);    
            }   
        }   
    }
    else {
        create_table();
        create_tr(id_item, title_item, price_item, img_item, quantity, quantity_step, upgrades);    
    }
    $('#bg_header .cart.special_cart_with_upgrades').addClass('visible');
}
