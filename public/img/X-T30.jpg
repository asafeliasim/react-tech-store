function focusEvent(obj){
  obj.parent().removeClass('validationError');
  obj.siblings('.validationErrorSpan').remove();
  validationFunc(obj);
}
function focusoutEvent(obj){
  validationFunc(obj);
  if(obj.parent().hasClass('validationError')){
    obj.after('<span class="validationErrorSpan">'+obj.data('error')+'</span>');
    obj.attr("aria-invalid", "true");
  }
  else{
    obj.attr("aria-invalid", "false");
  }
  var lang = 'he';
  if(lang != 'he'){
    $('span.validationErrorSpan').css('text-align','left');
  }
}
function validationFunc(obj){
  var input_value = obj.val();
  var re = /^\s*\S+.*/;
  if(obj.attr('name') == 'item_review[customer_email]'){
    re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
  }
  if (re.test(input_value)) {
    obj.parent().removeClass('validationError');
  } 
  else {
    obj.parent().addClass('validationError');
  }
}
