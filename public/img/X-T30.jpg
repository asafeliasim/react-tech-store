                if($(this).find(".group_values").height() >= max_height){
                    max_height = $(this).find(".group_values").height();
                }
            });
            if($("#wrapper .show_filters").height() == 30){
                $("#wrapper .show_filters").css("height",""+(max_height+50)+"px");
