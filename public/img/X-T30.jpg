<script>
var reviews_arr = [];
function Init(){
  reviews_arr = [];
  if (document.body.contains(document.getElementById('tabs'))) {
    document.getElementById('tabs').appendChild(document.getElementById('item_reviews'));
  };
  var reviewsHTML = "";
  for(var i=0; i<reviews_arr.length; i++){
    reviews_arr[i].title = decodeURI(reviews_arr[i].title);
    reviews_arr[i].content = decodeURI(reviews_arr[i].content);
    reviews_arr[i].customer_name = decodeURI(reviews_arr[i].customer_name);
    if(reviews_arr[i].answer != null && reviews_arr[i].answer != ""){
      reviews_arr[i].answer = decodeURI(reviews_arr[i].answer);
    }
    reviewsHTML += '<div id="wrap_review"><div id="item_reviews_content"><div class="review_title"><h3>'+
    reviews_arr[i].title+'</h3></div><div class="stars_rate"><span class="stars">'+
    createStars(reviews_arr[i].rates)+'</span><span class="rate_text">';
     
    reviewsHTML += reviews_arr[i].rates+' כוכבים</span></div>';
    
    if(reviews_arr[i].show_my_name == true){
       
      reviewsHTML += '<div class="review_user_name"><span>מאת '+
      reviews_arr[i].customer_name+'</span></div>';
      
    }
    reviewsHTML +='<div class="review_date"><span>'+
    reviews_arr[i].created_at.split(' ')[0]+'</span></div><div class="review_desc"><span>'+
    reviews_arr[i].content+'</span></div></div>';
    if(reviews_arr[i].answer != null && reviews_arr[i].answer != ""){
       
      reviewsHTML +='<div id="review_answer"><div class="answer_title">תשובה</div><div class="answer_status">תשובה</div><div class="answer_writer">מאת מנהל האתר</div><div class="answer_content"><p>'+reviews_arr[i].answer+'</p></div></div>';
      
    }
    reviewsHTML += '</div>'
  }
  document.getElementById("item_reviews").innerHTML += reviewsHTML;
}
function createStars(rate){
  var num_of_stars = Number(rate);
  var starsHTML = "";
  while(num_of_stars > 0){
    starsHTML +='<span></span>';
    num_of_stars --;
  }
  return starsHTML;
}

function calculateRate(){
  var sum = 0;
  for(var i=0; i<reviews_arr.length; i++){
    sum+=Number(reviews_arr[i].rates);
  }
//number of reviews
document.getElementById('reviews_number').innerHTML+= reviews_arr.length;
 
document.getElementById('reviews_text').innerHTML+=' חוות דעת';

//calculate average
var average = parseInt(sum / reviews_arr.length);
var span_total_stars = document.createElement("span");// Create a <span> node
 
if(reviews_arr.length == 0){
var textspan = document.createTextNode('(אין דירוג)');// Create a text node 
}
else{
var textspan = document.createTextNode('('+average+' מתוך 5 כוכבים)');// Create a text node
}

span_total_stars.appendChild(textspan);         
document.getElementById('rate_average').appendChild(span_total_stars);
//creates stars according to the average
var starsHTML_Header = createStars(average);
document.getElementById('stars_average').innerHTML+=starsHTML_Header;


}

function addReadMoreToReview(){
