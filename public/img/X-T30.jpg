function HTMLChanger(selector,attp_type,attr_value) {
	if (DEBUG) console.log(selector);
	if (DEBUG) console.log(attp_type);
	if (DEBUG) console.log(attr_value);
	selector.each(function(index, el) {
		if ($(this).length > 0 && $(this).attr(attp_type) == undefined  || $(this).attr(attp_type) == "") {
			$(this).attr(attp_type,attr_value);
		}			
	});
}

//in this case if display is "none" add or change css type and value
function CHANGE_css_if_css_is (selector,type,value,change_css_type,change_css_value) {
	$(selector).each(function(index, el) {
		if ($(this).css(type) == value) {
			$(this).css(change_css_type, change_css_value);
		}
	});
}


	

