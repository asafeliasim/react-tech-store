}
}
});
ret_arr.sort();
return ret_arr.join("»");
}
function strip_upgrade_text(str) {
if(str.match(/(^double_date)/g) == null) { // if its double_date - keeps it the same
if(str.match(/(^son)|(^line)/g) != null) // if its normal upgrade (not option)
str = str.replace(/^(son_item_upgrade_ids|line_item)(\[.*\]){1,2}$/g, 'u'); // replaces it with 'u'
else {
str = str.replace(/\[\]/g, ''); // removes the '[]'
str = str.replace(/^options(?:\_?\d*)\[/g, ''); // removes the 'options[' or 'options_123123['
if(str.charAt(str.length-1) == "]") //if there is ']' as last char
str = str.slice(0,-1); // removes it
}
}
return str;
}
